{
    "name": "vladimircatrici/shopify-api",
    "description": "Library to operate with Shopify data via REST API",
    "keywords": ["composer", "package", "shopify", "rest", "api"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Vladimir Catrici",
            "email": "vladimir.k89@gmail.com"
        }
    ],
    "type": "library",
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "require": {
		"php": "^5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "vlucas/phpdotenv": "^3.4",
        "guzzlehttp/guzzle": "^6.3.3",
        "symfony/phpunit-bridge": "^5.0"
    },
    "require-dev": {
		"phpunit/phpunit": "^5",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "VladimirCatrici\\Shopify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShopifyAPI\\Tests\\": "tests/"
        }
    }
}
